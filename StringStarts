public class Main {
    public static void main(String[] args) {
        String str1 = "Red is favorite color.";
        String str2 = "Red";
        
        System.out.println(str1 + " starts with " + str2 + "? " + str1.startsWith(str2));
        
        String str3 = "Orange is also my favorite color.";
        
        System.out.println(str3 + " starts with " + str2 + "? " + str3.startsWith(str2));
    }
}
